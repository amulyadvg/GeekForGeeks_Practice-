#include <iostream>
#include<math.h>
using namespace std;

int main() {
    int num = 8;
    int arr[20] = {1, 2, 3, 4, 6, 7, 8};
    
    int sum = 0;
    int sum_ele = (num*(num+1))/2;
    
    for (int i=0; i < num; i++) {
        sum += arr[i];
    }
    int miss_ele = sum_ele - sum;
    cout << "Missing element in array: " << miss_ele << endl;
return 0;
}
