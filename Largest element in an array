#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int a[20];
  int large = 0;
  int n;
  
  cout << "Enter number of elements: " << endl;
  cin >> n;
  
  cout << "Enter elements: " << endl;
  for (int i=0; i < n; i++) {
    cin >> a[i];
  }
  
  for (int i=0; i < n; i++) {
    if (a[i] > large) {
      large = a[i];
    }
  }
  
  cout << "Largest element in the array: " << large << endl;
return 0;
}
