#include <iostream>
#include<math.h>
using namespace std;

void reverseArray(int arr[], int num) {
    cout << "Reversed array: ";
    for (int j=num-1; j >= 0; j--) {
        cout << arr[j] << " ";
    } 
}

int main() {
    int num = 6;
    int arr[20] = {10, 20, 30, 40, 50, 60};
    
    cout << "Initial array: ";
    for (int i=0; i < num; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
    reverseArray(arr, num);
return 0;
}
