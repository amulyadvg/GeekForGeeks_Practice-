#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int arrA[20];
  int n;
  int sum = 0;
  
  cout << "Enter number of elements: " << endl;
  cin >> n;
  
  cout << "Enter elements: " << endl;
  for (int i=0; i < n; i++) {
    cin >> arrA[i];
  }
  
  for (int i=0; i < n; i++) {
    sum += arrA[i];
  }
  cout << endl;
  cout << "Sum of elements in the array: " << sum << endl;
return 0;
}
